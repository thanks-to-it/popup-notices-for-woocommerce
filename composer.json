{
  "name": "thanks-to-it/popup-notices-for-woocommerce",
  "description": "Turn your WooCommerce notices into popups",
  "keywords": [
    "wordpress",
    "woocommerce",
    "notice",
    "notices",
    "modal",
    "popup"
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/wpcodefactory/wpfactory-promoting-notice"
    }
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pablo Pacheco",
      "email": "pablo.sg.pacheco@gmail.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "require": {
    "wpfactory/wpfactory-promoting-notice": "*"
  },
  "extra": {
    "installer-paths": {
      "vendor/{$vendor}/{$name}": [
        "type:wordpress-plugin",
        "type:wordpress-muplugin"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "src/classes/"
    ],
    "exclude-from-classmap": [
      "/Tests/",
      "/test/",
      "/tests/"
    ]
  }
}